{
  "name": "vrwiz-server",
  "version": "0.1.0",
  "description": "Collaborative multiplayer VR client and server framework for data visualization powered by A-Frame and Socket.IO",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "clean": "rm -rf dist",
    "cert": "rm -rf .cert && mkdir .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'localhost'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CS1951T-AFrame/VRWiz.git"
  },
  "keywords": [
    "aframe",
    "vr",
    "multiplayer",
    "visualization",
    "multiplayer"
  ],
  "author": "CS1951T Students",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/CS1951T-AFrame/VRWiz/issues"
  },
  "homepage": "https://github.com/CS1951T-AFrame/VRWiz#readme",
  "dependencies": {
    "dotenv": "^16.0.0",
    "log4js": "^6.4.4",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  }
}
